                        <h1>Target <%= @target.name %></h1>
                        <p>Manage Provisioning Targets</p>
<!-- <div class="view"> -->
<div>
<p id="notice"><%= notice %></p>
<p id="error"><%= error %></p>
<p id="success"><%= success %></p>
</div>
  	<table>
  		<thead>
  			<th>Attribute</th>
			<th>Value	</th>
  		</thead>

  		<tbody>

			<% @target.attributes.each do |attribute| %> 
			<tr>
			    <td><%= attribute[0] %></td>
			    <td><%= attribute[1].to_s.gsub(/(assw[^=]*=[ ]*)([^\r]*).*$/, '\1*******') unless attribute[1].nil? %></td>
			</tr>
			<% end %>
			
			
		</tbody>
	</table>
</div>





